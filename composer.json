{
    "name": "opine/framework",
    "version": "1.3.12",
    "type": "library",
    "description": "Opine-PHP Micro-framework for integrating all Opine services",
    "keywords": [
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name"    : "Ryan Mahoney",
            "email"   : "ryan@virtuecenter.com",
            "homepage": "http://opine-php.org",
            "role"    : "Developer"
        }
    ],
    "require": {
        "php"                : ">=5.5.0",
        "opine/build"        : "2.0.8",
        "opine/bundle"       : "2.0.8",
        "opine/cache"        : "2.0.8",
        "opine/collection"   : "2.0.8",
        "opine/config"       : "3.0.8",
        "opine/container"    : "2.0.8",
        "opine/db"           : "2.0.8",
        "opine/field"        : "2.0.8",
        "opine/form"         : "2.0.8",
        "opine/handlebars"   : "2.0.8",
        "opine/helper"       : "2.0.8",
        "opine/http"         : "2.0.8",
        "opine/interfaces"   : "1.0.8",
        "opine/imageresizer" : "2.0.8",
        "opine/language"     : "2.0.8",
        "opine/layout"       : "2.0.8",
        "opine/mail"         : "2.0.8",
        "opine/person"       : "2.0.8",
        "opine/pubsub"       : "2.0.8",
        "opine/queue"        : "2.0.8",
        "opine/route"        : "3.0.8",
        "opine/search"       : "2.0.8",
        "opine/secret"       : "2.0.8",
        "opine/semantic-cm"  : "1.5.11",
        "opine/uploadwrapper": "1.0.8",
        "filp/whoops"        : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Opine\\": "src/"
        }
    }
}