{
    "name": "opine/framework",
    "type": "library",
    "description": "Opine-PHP Micro-framework for integrating all Opine services",
    "keywords": [
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name"    : "Ryan Mahoney",
            "email"   : "ryan@virtuecenter.com",
            "homepage": "http://opine-php.org",
            "role"    : "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php"               : ">=5.5.0",
        "opine/build"       : "dev-master",
        "opine/bundle"      : "1.9.0",
        "opine/cache"       : "2.0.2",
        "opine/collection"  : "dev-master",
        "opine/config"      : "dev-master",
        "opine/container"   : "dev-master",
        "opine/db"          : "dev-master",
        "opine/field"       : "dev-master",
        "opine/form"        : "dev-master",
        "opine/handlebars"  : "dev-master",
        "opine/helper"      : "dev-master",
        "opine/http"        : "dev-master",
        "opine/interfaces"  : "1.0.0",
        "opine/imageresizer": "dev-master",
        "opine/language"    : "dev-master",
        "opine/layout"      : "dev-master",
        "opine/mail"        : "dev-master",
        "opine/person"      : "dev-master",
        "opine/pubsub"      : "dev-master",
        "opine/queue"       : "dev-master",
        "opine/route"       : "3.0.1",
        "opine/search"      : "dev-master",
        "opine/secret"      : "dev-master",
        "opine/semantic-cm" : "1.5.5",
        "filp/whoops"       : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Opine\\": "src/"
        }
    }
}
