        "opine/build"        : "~2.0.0",
        "opine/bundle"       : "~2.0.0",
        "opine/cache"        : "~2.0.0",
        "opine/collection"   : "~2.0.0",
        "opine/config"       : "~3.0.0",
        "opine/container"    : "~2.0.0",
        "opine/db"           : "~2.0.0",
        "opine/field"        : "~2.0.0",
